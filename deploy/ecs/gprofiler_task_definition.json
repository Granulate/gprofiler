{
    "requiresCompatibilities": [
        "EC2"
    ],
    "containerDefinitions": [
        {
            "name": "granulate-gprofiler",
            "image": "index.docker.io/granulate/gprofiler:latest",
            "memory": "1024",
            "memoryReservation": "256",
            "cpu": "100",
            "essential": true,
            "command": [
                "-cu",
                "--token=<TOKEN>",
                "--service-name=<SERVICE NAME>"
            ],
            "user": "0:0",
            "privileged": true
        }
    ],
    "networkMode": null,
    "pidMode": "host",
    "ipcMode": "host",
    "family": "granulate-gprofiler"
}
